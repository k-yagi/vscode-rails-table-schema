{
  "name": "rails-table-schema",
  "displayName": "Rails Table Schema",
  "description": "Displays the table schema of the model",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "rails-table-schema.goToSchema",
        "title": "Rails Table Schema: Open schema.rb"
      }
    ],
    "keybindings": [
      {
        "command": "rails-table-schema.goToSchema",
        "key": "ctrl+alt+s",
        "mac": "cmd+alt+s",
        "when": "editorTextFocus && editorLangId == 'ruby'"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "16.x",
    "@types/pluralize": "^0.0.29",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "pluralize": "^8.0.0"
  }
}
